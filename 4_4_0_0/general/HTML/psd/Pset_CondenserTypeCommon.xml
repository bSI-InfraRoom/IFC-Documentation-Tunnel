      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Refrigerant Class</NameAlias>
        <NameAlias lang="fr-FR">ClasseRéfrigérant</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
