    <PropertyDef ifdguid="d79859eeada64f9cac636c57c5aa8841">
      <Name>InjectionPackerType</Name>
      <Definition>Equipment use to uphold pressure during injection.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_InjectionPackerType">
            <EnumItem>SINGLE</EnumItem>
            <EnumItem>DOUBLE</EnumItem>
            <EnumItem>USERDEFINED</EnumItem>
            <EnumItem>NOTDEFINED</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>SINGLE</Name>
              <Definition>Single seal to restrict the flow of grout in one direction, volume after packer is grouted.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>DOUBLE</Name>
              <Definition>Double seal to restrict the flow of grout in two directions, volume between packers is grouted.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>USERDEFINED</Name>
              <NameAliases>
                <NameAlias lang="en">User Defined</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>NOTDEFINED</Name>
              <NameAliases>
                <NameAlias lang="en">Notdefined</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
