<?xml version="1.0" encoding="utf-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ifdguid="69f627a930cb42a8b0f592a0259ab9f8" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4X4" />
  <Name>Pset_ImprovedGroundTypeInjectionGrouted</Name>
  <Definition>Properties describing improved ground of type injection grouted.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcImprovedGround/INJECTIONGROUTED</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcImprovedGround/INJECTIONGROUTED</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="29746210add846638f89e96105ae2577">
      <Name>InjectionPhase</Name>
      <Definition>Specifies when injection is performed.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_InjectionPhase">
            <EnumItem>PRE_INJECTION</EnumItem>
            <EnumItem>POST_INJECTION</EnumItem>
            <EnumItem>CONCURRENT_INJECTION</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>PRE_INJECTION</Name>
              <Definition>Injection is performed before excavation.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>POST_INJECTION</Name>
              <Definition>Injection is performed after excavation.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>CONCURRENT_INJECTION</Name>
              <Definition>Injection is performed during excavation.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <Definition>Value has not been specified</Definition>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="afc7b7c9c8fc4004ae65286ea8efd90d">
      <Name>InjectionDistributionEquipment</Name>
      <Definition>Equipment used for distributing the grout.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_InjectionDistributionEquipment">
            <EnumItem>INJECTIONHOSE</EnumItem>
            <EnumItem>INJECTIONTUBE</EnumItem>
            <EnumItem>PETROLEUMPIPE</EnumItem>
            <EnumItem>TUBESAMANCHETTE</EnumItem>
            <EnumItem>PILLOW</EnumItem>
            <EnumItem>BULLFLEX</EnumItem>
            <EnumItem>USERDEFINED</EnumItem>
            <EnumItem>NOTDEFINED</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>INJECTIONHOSE</Name>
              <Definition>Flexible conduit for conveying grout from the injection source to the targeted area, enabling precise and controlled grout placement.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>INJECTIONTUBE</Name>
              <Definition>Rigid or semi-rigid tube designed for delivering grout into specific locations, facilitating accurate injection, particularly in confined spaces.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>PETROLEUMPIPE</Name>
              <Definition>Pipe adapted for grouting purposes, resistant to substances in the grout, commonly used in scenarios requiring compatibility with petroleum-based grouting materials.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>TUBESAMANCHETTE</Name>
              <Definition>Grouting tube equipped with sleeves for controlled injections, providing a method for staged or zonal grouting, enhancing precision in treatment.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>PILLOW</Name>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>BULLFLEX</Name>
              <Definition>Textile formwork system a closed textile countainer is filled with grout to form a barrier or seal an opening.</Definition>
              <NameAliases />
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>USERDEFINED</Name>
              <NameAliases>
                <NameAlias lang="en">User Defined</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>NOTDEFINED</Name>
              <NameAliases>
                <NameAlias lang="en">Notdefined</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="df216e1b6e8949508e21caa0b85c49d1">
