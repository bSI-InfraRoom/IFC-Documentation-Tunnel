      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="1e14f29cfa794479b5038e902259335b">
      <Name>SO42-</Name>
      <Definition>Content of SO4 2- in the water sample.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
