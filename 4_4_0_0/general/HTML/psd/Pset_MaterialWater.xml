      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="58e417cdbd0a40eba448feaeaf3e4b02">
      <Name>HCO3-</Name>
      <Definition>Content of HCO3- in the water sample.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcReal" />
