      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="7ca2da32921b41a8ad9592b5f120fe41">
      <Name>Na+</Name>
      <Definition>Content of Na+ in the water sample.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcReal" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="adc0575b78974faf89e878c4a594cc0e">
      <Name>K+</Name>
      <Definition>Content of K+ in the water sample.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
